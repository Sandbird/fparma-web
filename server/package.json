{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit & npm run test:db",
    "test:unit": "jest -c config/jest.unit.json",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:db": "jest -c config/jest.db.json --runInBand",
    "test:db:watch": "jest -c config/jest.db.json --runInBand --watch",
    "setup:devdb": "docker-compose up -d --force-recreate",
    "lint:fix": "prettier --config ../.prettierrc --write ./src/**/*.ts",
    "start:devdb": "docker-compose start",
    "start": "ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "ts-node": "^7.0.1",
    "typeorm": "0.2.7",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.9.3",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.4"
  }
